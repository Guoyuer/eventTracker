@startuml
'https://plantuml.com/class-diagram

'abstract class AbstractList
'abstract AbstractCollection
'interface List
'interface Collection
'
'List <|-- AbstractList
'Collection <|-- AbstractCollection
'
'Collection <|- List
'AbstractCollection <|- AbstractList
'AbstractList <|-- ArrayList
'
'class ArrayList {
'Object[] elementData
'size()
'}
'
'enum TimeUnit {
'DAYS
'HOURS
'MINUTES
'}
entity Record{
    *id
    --
    *eventId
    startTime
    endTime
    value
}

entity Event{
    *id
    --
    *name
    description
    *careTime
    lastRecordId: FK<Record.id>
    unit: FK<Unit.name>
    sumVal
    sumTime
}

entity Unit{
    *id
    --
    *name
}

Unit "0 or 1" |o-- Event

@enduml